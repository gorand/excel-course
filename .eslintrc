{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "eol-last": "off",
        "linebreak-style": "off",
        "indent": ["error", 4],
        "object-curly-spacing":"off",
        "require-jsdoc": "off",
        "arrow-parens": "off",
        "no-trailing-spaces": "off",
        "max-len": ["error", 120],
        "operator-linebreak": "off"
    },
    "extends": ["eslint:recommended", "google"]
}